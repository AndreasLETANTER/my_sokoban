#!/bin/bash

myArray=('*/#*#' '*/*~' '*/*pain_au_chocolat*' '*/*.o' '*/*.a' '*/*.so' '*/*.gcno' '*/*.gcda' '*/*.gcov' '#*#' '*~' '*pain_au_chocolat*' '*.o' '*.a' '*.so' '*.gcno' '*.gcda' '*.gcov')
EXECUTABLES="a.out"
IFS=','
read -a strarr <<< "$EXECUTABLES"
for name in "${strarr[@]}"; do
    file_count=$(find "." -perm 775 -print | grep "$name" | wc -l)
    if [[ $file_count -gt 0 ]]; then
        echo "::error title="dqzdzq fdqz: $str#L1"::$str"
    fi
done